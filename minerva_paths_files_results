# age groups based on https://www.nature.com/articles/nature10523
neonatal = 0-1years
childhood = 2-11years
adolescence = 12-19years
young adulthood = 20-39years
middle adulthood = 40-59years
late adulthood = > 60years

# usage
pb=readRDS("/sc/arion/projects/psychAD/NPS-AD/freeze2_rc/pseudobulk/AGING_2024-02-01_22_23_PB_SubID_subclass.RDS")
colData(pb)$groups="NA"
colData(pb)$groups[colData(pb)$Age<1]="Neonatal"
colData(pb)$groups[colData(pb)$Age>=1 & colData(pb)$Age<12]="Childhood"
colData(pb)$groups[colData(pb)$Age>=12 & colData(pb)$Age<20]="Adolescence"
colData(pb)$groups[colData(pb)$Age>=20 & colData(pb)$Age<40]="Young_Adulthood"
colData(pb)$groups[colData(pb)$Age>=40 & colData(pb)$Age<60]="Middle_Adulthood"
colData(pb)$groups[colData(pb)$Age>=60]="Late_Adulthood"

table(colData(pb)$groups)

     Adolescence        Childhood   Late_Adulthood Middle_Adulthood
              34               12               83               95
        Neonatal  Young_Adulthood
              12               55

                  Source
groups              H  M
  Neonatal         12  0
  Childhood        12  0
  Adolescence      34  0
  Young_Adulthood  45 10
  Middle_Adulthood 60 35
  Late_Adulthood   15 68


